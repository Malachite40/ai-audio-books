{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "generate"
      ]
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false,
      "outputs": [
        "packages/database/generated/**"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "generate"
      ]
    }
  }
}
